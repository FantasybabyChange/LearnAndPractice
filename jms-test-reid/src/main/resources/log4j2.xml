<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="info" name="fantasybabyLog" packages="com.fantasybaby">
  <Appenders>
    <Console name="Console" target="SYSTEM_OUT">
      <PatternLayout pattern="%d{yyyy-MM-dd HH:mm:ss.SSS}	%p	%t	%c{1}:%L	%m%n"/>
    </Console>

  </Appenders>
  <Loggers>
  	<logger name="com.fantasybaby" level="info" />
	  	<Root level="info">
	  		<AppenderRef ref="Console" />
	     </Root>
     <!-- <Root level="info">
         <AppenderRef ref="stringAppender" />
     </Root> -->
     
  </Loggers>
</Configuration>

